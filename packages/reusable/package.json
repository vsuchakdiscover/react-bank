{
  "name": "reusable",
  "private": true,
  "version": "0.0.1",
  "description": "Discover reusable components",
  "author": "coryhouse",
  "main": "lib",
  "module": "lib-es",
  "jsnext:main": "lib-es",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test:watch": "react-scripts test --env=jsdom",
    "build:lib": "rollup -c",
    "start": "run-p lint:watch start:storybook",
    "start:storybook": "start-storybook -p 9009 -s public --quiet",
    "test": "react-scripts test",
    "test:ci": "cross-env CI=true react-scripts test && npm run cy:ci",
    "storybook": "npm start",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "lint:watch": "esw src",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "build-storybook": "build-storybook -s public --quiet --loglevel silent",
    "cy": "start-server-and-test start http://localhost:9009 cy:open",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:ci": "node tools/setMaxListeners && start-server-and-test start:storybook http://localhost:9009 cy:run"
  },
  "peerDependencies": {
    "bootstrap": "^4.4.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@storybook/addon-a11y": "^5.3.9",
    "@storybook/addon-actions": "^5.3.9",
    "@storybook/addon-info": "^5.3.9",
    "@storybook/addon-knobs": "^5.3.9",
    "@storybook/addon-links": "^5.3.9",
    "@storybook/addon-notes": "^5.3.9",
    "@storybook/addon-viewport": "^5.3.9",
    "@storybook/addons": "^5.3.9",
    "@storybook/preset-create-react-app": "^1.5.2",
    "@storybook/react": "^5.3.9",
    "@svgr/rollup": "^5.1.0",
    "@testing-library/cypress": "^6.0.0",
    "@testing-library/jest-dom": "^5.0.2",
    "@testing-library/react": "^9.4.0",
    "bootstrap": "^4.4.1",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.0",
    "cypress": "4.4.0",
    "eslint": "^6.8.0",
    "eslint-watch": "^6.0.1",
    "json-server": "^0.15.1",
    "node-sass": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.12.0",
    "react-animate-height": "^2.0.20",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "react-test-renderer": "^16.12.0",
    "rollup": "^1.30.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-delete": "^1.2.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.5",
    "rollup-plugin-url": "^3.0.1",
    "start-server-and-test": "^1.10.8",
    "storybook-readme": "^5.0.8",
    "wait-on": "^4.0.0"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "@reach/component-component": "^0.7.4",
    "@reach/dialog": "^0.7.4",
    "@reach/menu-button": "^0.7.4",
    "@reach/portal": "^0.7.4",
    "@reach/utils": "^0.7.4",
    "downshift": "^5.1.1",
    "is-url-external": "^1.0.3",
    "match-sorter": "^4.0.2",
    "react-focus-lock": "^2.2.1",
    "react-table": "7.0.0",
    "react-text-mask": "^5.4.3",
    "react-to-print": "^2.5.1"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ],
      "@babel/preset-react"
    ]
  }
}
